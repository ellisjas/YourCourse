<%= form_for @message, url: create_message_url, html: { class: 'contact-form' } do |f| %>
  <%= notice %>
  <%= f.text_field :name, placeholder: 'name' %>
  <%= f.email_field :email, placeholder: 'email' %>
  <%= f.text_area :body, placeholder: 'body' %>
  <%= f.submit 'Send', class: 'btn contact-btn' %>
<% end %>