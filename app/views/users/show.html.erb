<% provide(:title, @user.name) %>
<h1>Coordinator: <%= @user.name %></h1>
<hr />

<div class="container">
  <div class="row">
    <div class="col-md-2 col-md-offset-5">
      <%= gravatar_for @user %>
    </div>
  </div>
</div>

<hr />
<div class="container">
  <div class="row">
    <div>
      <ul class="courses">
        <%= render @courses %>
      </ul>
    </div>
  </div>
</div>